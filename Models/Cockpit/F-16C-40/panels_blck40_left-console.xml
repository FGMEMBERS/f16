<?xml version="1.0"?>

<PropertyList>

 <path>panels_blck40_left-console.ac</path>

 <!-- PICK ANIMATIONS -->

  <!-- External Lighting Panel -->
 
   <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>HS_MASTER_NORM</object-name>
    <action>
     <button>0</button>
     <repeatable>true</repeatable>
     <binding>
      <command>nasal</command>
      <script>
                print("EPIC01, call func");
                f16_lighting.toggleMaster();
      </script>
     </binding>
    </action>
   </animation>
   
