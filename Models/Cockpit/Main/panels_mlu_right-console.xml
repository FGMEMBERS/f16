<?xml version="1.0"?>

<PropertyList>

 <path>panels_mlu_right-console.ac</path>

    <animation>
        <type>knob</type>
        <object-name>inst-pnl-flood-knob</object-name>
        <property>controls/lighting/lighting-panel/flood-inst-pnl-knob</property>
        <offset-deg>135</offset-deg>
        <factor>-222</factor>
        <axis>
            <object-name>inst-pnl-flood-knob-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/lighting/lighting-panel/flood-inst-pnl-knob</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>inst-pnl-primary-knob</object-name>
        <property>controls/lighting/lighting-panel/pri-inst-pnl-knob</property>
        <offset-deg>135</offset-deg>
        <factor>-222</factor>
        <axis>
            <object-name>inst-pnl-primary-knob-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/lighting/lighting-panel/pri-inst-pnl-knob</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>console-flood-knob</object-name>
        <property>controls/lighting/lighting-panel/console-flood-knob</property>
        <offset-deg>155</offset-deg>
        <factor>-330</factor>
        <axis>
            <object-name>console-flood-knob-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/lighting/lighting-panel/console-flood-knob</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>data-entry-display-knob</object-name>
        <property>controls/lighting/lighting-panel/data-entry-display</property>
        <offset-deg>135</offset-deg>
        <factor>-222</factor>
        <axis>
            <object-name>data-entry-display-knob-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/lighting/lighting-panel/data-entry-display</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>console-primary-knob</object-name>
        <property>controls/lighting/lighting-panel/console-primary-knob</property>
        <offset-deg>135</offset-deg>
        <factor>-222</factor>
        <axis>
            <object-name>console-primary-knob-axis</object-name>
        </axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>controls/lighting/lighting-panel/console-primary-knob</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>material</type>
        <object-name>RIGHT_CONSOLE</object-name>
        <emission>
            <factor-prop>controls/lighting/lighting-panel/flood-inst-pnl</factor-prop>
            <red>0.45</red>
            <green>0.34</green>
            <blue>0.18</blue>
        </emission>
    </animation>
    <effect>
        <name>models-effects-f16panel-left</name>
        <inherits-from>Aircraft/f16/Models/Effects/f16panel-left</inherits-from>
        <parameters>
            <implicit-lightmap-threshold-low type="float">0.2</implicit-lightmap-threshold-low>
            <implicit-lightmap-threshold-high type="float">0.7</implicit-lightmap-threshold-high>
        </parameters>
        <object-name>LIGHTING_1</object-name>
        <object-name>DTU_1</object-name>
        <object-name>GROWNTH.104_1</object-name>
        <object-name>OXYGEN_REGULATOR_1</object-name>
        <object-name>GROWNTH.103_1</object-name>
        <object-name>AVIONICS_POWER_1</object-name>
        <object-name>ANTI_ICE_1</object-name>
        <object-name>GROWNTH.102_1</object-name>
        <object-name>KY_58_1</object-name>
        <object-name>AIR_COND_1</object-name>
        <object-name>GROWNTH.102_1</object-name>
        <object-name>NUCLEAR_CONSENT_1</object-name>
        <object-name>HUD_1</object-name>
        <object-name>SNSR_PWR_1</object-name>
        <object-name>VOICE_MESSAGE_1</object-name>
        <object-name>ZEROIZE_1</object-name>
    </effect>
    <effect>
        <name>models-effects-f16panel-left</name>
        <inherits-from>Aircraft/f16/Models/Effects/f16panel-knob</inherits-from>
        <object-name>inst-pnl-flood-knob</object-name>
        <object-name>inst-pnl-flood-knob-axis</object-name>
        <object-name>inst-pnl-primary-knob</object-name>
        <object-name>inst-pnl-primary-knob-axis</object-name>
        <object-name>console-flood-knob</object-name>
        <object-name>console-flood-knob-axis</object-name>
        <object-name>data-entry-display-knob</object-name>
        <object-name>data-entry-display-knob-axis</object-name>
        <object-name>console-primary-knob</object-name>
        <object-name>console-primary-knob-axis</object-name>
        <object-name>inst-pnl-flood-knob</object-name>
        <!-- these needs to be animated
        <object-name>tas-switch</object-name>
        <object-name>altrad-switch</object-name>
        <object-name>autobrt-switch</object-name>
        <object-name>test-step-switch</object-name>
        <object-name>w_vah-switch</object-name>
        <object-name>att_fpm-switch</object-name>
        <object-name>deddata-switch</object-name>
        <object-name>nuke-consent-switch</object-name>
        <object-name>nuke-consent-cover</object-name>-->
    </effect>

</PropertyList>
