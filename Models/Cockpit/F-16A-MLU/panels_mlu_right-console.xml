<?xml version="1.0"?>

<PropertyList>

 <path>panels_mlu_right-console.ac</path>

 <!-- PICK ANIMATIONS -->

  <!-- Lighting Panel -->

   <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>HS_PRI_INST_PNL_UP</object-name>
    <action>
     <button>0</button>
     <repeatable>true</repeatable>
     <binding>
      <command>nasal</command>
      <script>
                f16_switches.togglePriInstPnlUp();
      </script>
     </binding>
    </action>
   </animation>

