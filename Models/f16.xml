<?xml version="1.0"?>

<PropertyList>

 <path>f16.ac</path>

 <offsets>
   <z-m>0.0</z-m>
 </offsets>

 <animation>
  <type>blend</type>
  <object-name>internal_flame</object-name>
  <property>/engines/engine[0]/EGT_degC</property>
  <factor>-0.00135135135135</factor>
  <offset>1.5</offset>
 </animation>

 <animation>
  <type>select</type>
  <object-name>external_flame</object-name>
  <condition>
   <equals>
    <property>/engines/engine[0]/augmentation</property>
    <value>true</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>right_upper_aileron</object-name>
  <object-name>right_lower_aileron</object-name>
  <property>/surface-positions/left-aileron-pos-norm</property>
  <factor>-30</factor>
  <center>
   <x-m>1.81</x-m>
   <y-m>4.12</y-m>
   <z-m>0.05</z-m>
  </center>
  <axis>
   <x>0.25</x>
   <y>0.84</y>
   <z>0.02</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>left_upper_aileron</object-name>
  <object-name>left_lower_aileron</object-name>
  <property>/surface-positions/left-aileron-pos-norm</property>
  <factor>-30</factor>
  <center>
   <x-m>1.81</x-m>
   <y-m>-4.12</y-m>
   <z-m>0.05</z-m>
  </center>
  <axis>
   <x>0.25</x>
   <y>-0.84</y>
   <z>-0.02</z>
  </axis>
 </animation>

