<?xml version="1.0"?>

<PropertyList>

 <model>
  <name>Cockpit</name>
  <path>Aircraft/f16/Models/cockpit.xml</path>
 </model>

 <model>
  <name>Aircraft Body</name>
  <path>Aircraft/f16/Models/f16.ac</path>
 </model>

 <offsets>
  <z-m>0.15</z-m>
  <pitch-deg>0.3</pitch-deg>
 </offsets>

 <model>
  <name>new_nozzle</name>
  <path>Aircraft/f16/Models/nozzle.xml</path>
  <offsets>
   <x-m>4.617</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </offsets>
 </model>

 <!-- group all position lights -->
 <animation>
  <name>pos_lights</name>
  <object-name>green_pos_light2</object-name>
  <object-name>red_pos_light2</object-name>
  <object-name>aft_pos_light1</object-name>
  <object-name>aft_pos_light2</object-name>
 </animation>
 
 <animation>
  <name>pos_lights_lit</name>
  <object-name>green_pos_light1</object-name>
  <object-name>red_pos_light1</object-name>
  <object-name>wing_red_pos_light</object-name>
  <object-name>wing_green_pos_light</object-name>
 </animation>

