<?xml version="1.0"?>

<PropertyList>

 <path>f16.ac</path>
 <offsets>
  <z-m>0.15</z-m>
  <pitch-deg>0.3</pitch-deg>
 </offsets>

 <!-- sort transparent objects -->
 <animation>
  <object-name>canopy_glass</object-name>
  <object-name>hud_glass</object-name>
  <object-name>canopy_back_insiude</object-name>
  <object-name>canopy_back_outside</object-name>
  <object-name>klu_markings</object-name>
  <object-name>us_markings</object-name>
 </animation>

 <!-- group all position lights -->
 <animation>
  <name>pos_lights</name>
  <object-name>green_pos_light2</object-name>
  <object-name>red_pos_light2</object-name>
  <object-name>aft_pos_light1</object-name>
  <object-name>aft_pos_light2</object-name>
 </animation>
 
 <animation>
  <name>pos_lights_lit</name>
  <object-name>green_pos_light1</object-name>
  <object-name>red_pos_light1</object-name>
  <object-name>wing_red_pos_light</object-name>
  <object-name>wing_green_pos_light</object-name>
 </animation>

<!-- disable shadows for these objects -->
 <animation>
  <type>noshadow</type>
  <object-name>us_markings</object-name>
  <object-name>klu_markings</object-name>
  <object-name>pos_lights_lit</object-name>
  <object-name>pos_lights</object-name>
  <object-name>afterburner</object-name>
  <object-name>turbine</object-name>
  <object-name>fan</object-name>
  <object-name>internal_flame</object-name>
  <object-name>external_flame</object-name>
  <object-name>low_formation_light</object-name>
  <object-name>fwd_formation_light</object-name>
  <object-name>anti_collision_strobe_off</object-name>
  <object-name>hud_glass</object-name>
  <object-name>lhicondenscone</object-name>
  <object-name>rhicondenscone</object-name>
  <object-name>lhocondenscone</object-name>
  <object-name>rhocondenscone</object-name>
 </animation>

 <animation>
  <type>alpha-test</type>
  <alpha-factor>0.01</alpha-factor>
 </animation>

