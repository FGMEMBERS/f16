 <animation>
  <type>select</type>
  <object-name>down</object-name>
  <condition>
    <property>controls/gear/gear-down</property>
    <greater-than>
     <property>velocities/glideslope</property>
     <value>-0.192</value>
    </greater-than>
   </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>steady</object-name>
  <condition>
    <property>controls/gear/gear-down</property>
    <greater-than>
     <property>velocities/glideslope</property>
     <value>-0.2618</value>
    </greater-than>
    <less-than>
     <property>velocities/glideslope</property>
     <value>-0.192</value>
    </less-than>
   </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>up</object-name>
  <condition>
    <property>controls/gear/gear-down</property>
    <less-than>
     <property>velocities/glideslope</property>
     <value>-0.2618</value>
    </less-than>
   </condition>
 </animation>

