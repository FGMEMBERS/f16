 <!-- AOA indexer animations -->
 <animation>
  <type>select</type>
  <object-name>down</object-name>
  <condition>
    <property>controls/gear/gear-down</property>
    <greater-than>
     <property>orientation/alpha-deg</property>
     <value>15.0</value>
    </greater-than>
   </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>steady</object-name>
  <condition>
    <property>controls/gear/gear-down</property>
    <greater-than>
     <property>orientation/alpha-deg</property>
     <value>11.0</value>
    </greater-than>
    <less-than>
     <property>orientation/alpha-deg</property>
     <value>15.0</value>
    </less-than>
   </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>up</object-name>
  <condition>
    <property>controls/gear/gear-down</property>
    <less-than>
     <property>orientation/alpha-deg</property>
     <value>11.0</value>
    </less-than>
   </condition>
 </animation>

 <!-- AAR info animations -->
 <animation>
  <type>select</type>
  <object-name>rdy</object-name>
  <condition>
   <equals>
    <property type="bool">/systems/refuel/serviceable</property>
    <value>true</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>ar_nws</object-name>
  <condition>
   <equals>
    <property type="bool">/systems/refuel/contact</property>
    <value>true</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>disc</object-name>
  <condition>
   <equals>
    <property type="bool">/systems/refuel/contact</property>
    <value>false</value>
   </equals>
  </condition>
 </animation>

</PropertyList>
