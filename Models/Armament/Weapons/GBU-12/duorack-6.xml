<?xml version="1.0" encoding="iso-8859-1"?>
  
<PropertyList> 

    <path>Aircraft/f16/Models/Armament/Triplerack/duorack.ac</path>

    <model>
      <name>Station6_GBU12_left</name>
      <path>Aircraft/f16/Models/Armament/Weapons/GBU-12/gbu12.xml</path>
      <offsets>
        <x-m>-0.35</x-m>
        <y-m>-0.432802</y-m>
        <z-m>-0.231045</z-m>
        <pitch-deg>0</pitch-deg>
        <roll-deg>00</roll-deg>
      </offsets>
    </model>

    <model>
      <name>Station6_GBU12_right</name>
      <path>Aircraft/f16/Models/Armament/Weapons/GBU-12/gbu12.xml</path>
      <offsets>
        <x-m>-0.35</x-m>
        <y-m>0.432802</y-m>
        <z-m>-0.231045</z-m>
        <pitch-deg>0</pitch-deg>
        <roll-deg>00</roll-deg>
      </offsets>
    </model>

    <animation>
      <type>select</type>
      <object-name>Station6_GBU12_left</object-name>
      <condition><and>
        <equals>
          <property>payload/armament/station/id-5-type</property>
          <value>GBU-12</value>
        </equals>
        <greater-than>
          <property>payload/armament/station/id-5-count</property>
          <value>0</value>
        </greater-than></and>
      </condition>
     </animation>

     <animation>
      <type>select</type>
      <object-name>Station6_GBU12_right</object-name>
      <condition>
        <and>
        <equals>
          <property>payload/armament/station/id-5-type</property>
          <value>GBU-12</value>
        </equals>
        <greater-than>
          <property>payload/armament/station/id-5-count</property>
          <value>1</value>
        </greater-than>
      </and>
      </condition>
     </animation>

</PropertyList> 
