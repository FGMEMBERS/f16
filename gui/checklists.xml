<?xml version="1.0"?>

<PropertyList>

    <checklist>
        <title>Start from cold and dark</title>
        <item>
            <name>ENG FEED</name>
            <value>NORM</value>
            <condition>
                <equals>
                    <property>f16/engine/feed</property>
                    <value>1</value>
                </equals>
            </condition>
            <binding>
                <command>property-assign</command>
                <property>f16/engine/feed</property>
                <value>1</value>
            </binding>
        </item>
        <item>
            <name>EPU</name>
            <value>NORM</value>
            <condition>
                <equals>
                    <property>fdm/jsbsim/elec/switches/epu</property>
                    <value>1</value>
                </equals>
            </condition>
            <binding>
                <command>property-assign</command>
                <property>fdm/jsbsim/elec/switches/epu</property>
                <value>1</value>
            </binding>
        </item>
        <item>
            <name>ELEC</name>
            <value>BATT</value>
            <!--<marker>
                <x-m>-3.9554</x-m>
                <y-m>-0.0595</y-m>
                <z-m>0.0686</z-m>
                <scale>2.4711</scale>
            </marker>-->
            <condition>
                <equals>
                    <property>fdm/jsbsim/elec/switches/main-pwr</property>
                    <value>1</value>
                </equals>
            </condition>
            <binding>
                <command>property-assign</command>
                <property>fdm/jsbsim/elec/switches/main-pwr</property>
                <value>1</value>
            </binding>
        </item>
        <item>
            <name>MAL and IND LTS</name>
            <value>ON</value>
            <condition>
                <equals>
                    <property>controls/test/test-panel/mal-ind-lts</property>
                    <value>1</value>
                </equals>
            </condition>
            <binding>
                <command>nasal</command>
                <script>f16_switches.toggleMalIndLts()</script>
            </binding>
        </item>
        <item>
            <name>MAL and IND LTS</name>
            <value>OFF</value>
            <condition>
                <equals>
                    <property>controls/test/test-panel/mal-ind-lts</property>
                    <value>0</value>
                </equals>
            </condition>
            <binding>
                <command>property-assign</command>
                <property>controls/test/test-panel/mal-ind-lts</property>
                <value>0</value>
            </binding>
        </item>
        <item>
            <name>ELEC</name>
            <value>MAIN PWR</value>
            <condition>
                <equals>
                    <property>fdm/jsbsim/elec/switches/main-pwr</property>
                    <value>2</value>
                </equals>
            </condition>
            <binding>
                <command>property-assign</command>
                <property>fdm/jsbsim/elec/switches/main-pwr</property>
                <value>2</value>
            </binding>
        </item>
        <item>
            <name>JSF START (black switch on throttle)</name>
            <value>OFF</value>
            <condition>
                <equals>
                    <property>f16/engine/jsf-start</property>
                    <value>0</value>
                </equals>
            </condition>
            <binding>
                <command>property-assign</command>
                <property>f16/engine/jsf-start</property>
                <value>0</value>
            </binding>
        </item>
        <item>
            <name>JET FUEL</name>
            <value>START 2</value>
            <condition>
                <equals>
                    <property>f16/engine/jet-fuel</property>
                    <value>1</value>
                </equals>
            </condition>
            <binding>
                <command>property-assign</command>
                <property>f16/engine/jet-fuel</property>
                <value>1</value>
            </binding>
        </item>
        <item>
            <name>Wait for RPM</name>
            <value>N2 25%</value>
            <condition>
                <greater-than>
                    <property>engines/engine[0]/n2</property>
                    <value>25</value>
                </greater-than>
            </condition>
        </item>
        <item>
            <name>JSF START (black switch on throttle)</name>
            <value>ON</value>
            <condition>
                <equals>
                    <property>f16/engine/jsf-start</property>
                    <value>1</value>
                </equals>
            </condition>
            <binding>
                <command>property-assign</command>
                <property>f16/engine/jsf-start</property>
                <value>1</value>
            </binding>
        </item>
        <item>
            <name>Wait for engine</name>
            <value>Ready</value>
            <condition>
                <equals>
                    <property>engines/engine[0]/running</property>
                    <value>1</value>
                </equals>
            </condition>
        </item>
    </checklist>
</PropertyList>