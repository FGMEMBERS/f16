<?xml version="1.0" encoding="iso-8859-1"?>

<PropertyList>
  
  <name>payload</name>
  <modal type="bool">false</modal>
  <draggable>true</draggable>
  <resizable>true</resizable>
  <width>500</width>
  <layout>vbox</layout>
  
  <group>
    <layout>hbox</layout>
    <empty>
      <stretch>1</stretch>
    </empty>
    
    <text>
      <label>Payload quick setup</label>
    </text>
    
    <empty>
      <stretch>1</stretch>
    </empty>
    
    <button>
      <pref-width>16</pref-width>
      <pref-height>16</pref-height>
      <legend></legend>
      
      <default>1</default>
      <keynum>27</keynum>
      <border>2</border>
      <binding>
        <command>dialog-close</command>
      </binding>
      <binding>
        <command>dialog-apply</command>
      </binding>
    </button>
  </group>

  <hrule/>
    <group>
        <layout>table</layout>
        <text>
            <row>0</row>
            <col>0</col>
            <label>Air patrol</label>
        </text>
        <button>
            <row>0</row>
            <col>1</col>
            <legend>Mount</legend>
            <binding>
                <command>nasal</command>
                <script>pylons.a2a_patrol();</script>
            </binding>
        </button>

        <hrule>
            <row>1</row>
            <col>0</col>
            <colspan>4</colspan>
        </hrule>

        <text>
            <row>2</row>
            <col>0</col>
            <label>Air superiority</label>
        </text>
        <button>
            <row>2</row>
            <col>1</col>
            <legend>Mount</legend>
            <binding>
                <command>nasal</command>
                <script>pylons.a2a_super();</script>
            </binding>
        </button>

        <hrule>
            <row>3</row>
            <col>0</col>
            <colspan>4</colspan>
        </hrule>

        <text>
            <row>4</row>
            <col>0</col>
            <label>Combat Air Patrol</label>
        </text>
        <button>
            <row>4</row>
            <col>1</col>
            <legend>Mount</legend>
            <binding>
                <command>nasal</command>
                <script>pylons.a2a_cap();</script>
            </binding>
        </button>

        <hrule>
            <row>5</row>
            <col>0</col>
            <colspan>4</colspan>
        </hrule>

        <text>
            <row>6</row>
            <col>0</col>
            <label>Combat Air patrol (Extended)</label>
        </text>
        <button>
            <row>6</row>
            <col>1</col>
            <legend>Mount</legend>
            <binding>
                <command>nasal</command>
                <script>pylons.a2a_capext();</script>
            </binding>
        </button>

        <hrule>
            <row>7</row>
            <col>0</col>
            <colspan>4</colspan>
        </hrule>

        <layout>hbox</layout>
        <text>
            <row>8</row>
            <col>0</col>
            <label>Close Air Support</label>
        </text>
        <button>
            <row>8</row>
            <col>1</col>
            <legend>Mount</legend>
            <binding>
                <command>nasal</command>
                <script>pylons.a2g_cas();</script>
            </binding>
        </button>

        <hrule>
            <row>9</row>
            <col>0</col>
            <colspan>4</colspan>
        </hrule>

        <text>
            <row>10</row>
            <col>0</col>
            <label>Close Air Support (Extended)</label>
        </text>
        <button>
            <row>10</row>
            <col>1</col>
            <legend>Mount</legend>
            <binding>
                <command>nasal</command>
                <script>pylons.a2g_casext();</script>
            </binding>
        </button>

        <hrule>
            <row>11</row>
            <col>0</col>
            <colspan>4</colspan>
        </hrule>

        <text>
            <row>12</row>
            <col>0</col>
            <label>Guided A/G (config 1)</label>
        </text>
        <button>
            <row>12</row>
            <col>1</col>
            <legend>Mount</legend>
            <binding>
                <command>nasal</command>
                <script>pylons.a2g_guided1();</script>
            </binding>
        </button>

        <hrule>
            <row>13</row>
            <col>0</col>
            <colspan>4</colspan>
        </hrule>

        <text>
            <row>14</row>
            <col>0</col>
            <label>Guided A/G (config 2)</label>
        </text>
        <button>
            <row>14</row>
            <col>1</col>
            <legend>Mount</legend>
            <binding>
                <command>nasal</command>
                <script>pylons.a2g_guided2();</script>
            </binding>
        </button>

        <text>
            <row>0</row>
            <col>2</col>
            <label>Air Strike</label>
        </text>
        <button>
            <row>0</row>
            <col>3</col>
            <legend>Mount</legend>
            <binding>
                <command>nasal</command>
                <script>pylons.a2g_mix();</script>
            </binding>
        </button>

        <text>
            <row>2</row>
            <col>2</col>
            <label>Air to sea (Anti-ship)</label>
        </text>
        <button>
            <row>2</row>
            <col>3</col>
            <legend>Mount</legend>
            <binding>
                <command>nasal</command>
                <script>pylons.a2s_antiship();</script>
            </binding>
        </button>


        <text>
            <row>4</row>
            <col>2</col>
            <label>"Wild Weasel" (SEAD)</label>
        </text>
        <button>
            <row>4</row>
            <col>3</col>
            <legend>Mount</legend>
            <binding>
                <command>nasal</command>
                <script>pylons.a2g_sead();</script>
            </binding>
        </button>

        <text>
            <row>6</row>
            <col>2</col>
            <label>Offensive Counter Air</label>
        </text>
        <button>
            <row>6</row>
            <col>3</col>
            <legend>Mount</legend>
            <binding>
                <command>nasal</command>
                <script>pylons.a2g_oca();</script>
            </binding>
        </button>

        <text>
            <row>8</row>
            <col>2</col>
            <label>Ferry flight</label>
        </text>
        <button>
            <row>8</row>
            <col>3</col>
            <legend>Mount</legend>
            <binding>
                <command>nasal</command>
                <script>pylons.a2a_ferry();</script>
            </binding>
        </button>


        <text>
            <row>10</row>
            <col>2</col>
            <label>Ferry flight w/ cargo</label>
        </text>
        <button>
            <row>10</row>
            <col>3</col>
            <legend>Mount</legend>
            <binding>
                <command>nasal</command>
                <script>pylons.a2a_ferrycargo();</script>
            </binding>
        </button>

        <text>
            <row>12</row>
            <col>2</col>
            <label>Training</label>
        </text>
        <button>
            <row>12</row>
            <col>3</col>
            <legend>Mount</legend>
            <binding>
                <command>nasal</command>
                <script>pylons.a2a_training();</script>
            </binding>
        </button>

        <text>
            <row>14</row>
            <col>2</col>
            <label>Clean</label>
        </text>
        <button>
            <row>14</row>
            <col>3</col>
            <legend>Mount</legend>
            <binding>
                <command>nasal</command>
                <script>pylons.clean();</script>
            </binding>
        </button>
    </group>
</PropertyList>