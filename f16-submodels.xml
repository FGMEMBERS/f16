<?xml version="1.0"?>

<PropertyList>

  <submodel>
    <name>flares</name>
    <model>Aircraft/f16/Models/flare.xml</model>
    <trigger>ai/submodels/submodel[0]/flare-release</trigger>
    <speed>250.0</speed>	<!-- ft/sec -->
    <repeat>true</repeat>
    <delay>0.75</delay>
    <count>40</count>
    <x-offset>4.26</x-offset>
    <y-offset>1.0</y-offset>
    <z-offset>0.1</z-offset>
    <yaw-offset>90.0</yaw-offset>
    <life>5</life>
    <buoyancy>8</buoyancy>
    <eda>1.5</eda>
  </submodel>

  <submodel>
    <name>contrail</name>
    <model>Aircraft/f16/Models/particle.xml</model>
    <trigger>ai/submodels/submodel[0]/flare-release</trigger>
    <speed>250.0</speed>
    <repeat>true</repeat>
    <delay>0.25</delay>
    <yaw-offset>90.0</yaw-offset>
    <count>-1</count>
    <life>15</life>
    <wind>true</wind>
    <buoyancy>10</buoyancy>
    <eda>1000</eda>
  </submodel>

</PropertyList>
