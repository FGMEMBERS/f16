<?xml version="1.0"?>

<PropertyList include="f16-2dpanel-set.xml">

 <sim>
  <description>General Dynamics F-16AT (Falcon-21)</description>
  <aero>f16at</aero>

 
  <virtual-cockpit archive="y">true</virtual-cockpit>
  <panel>
   <visibility archive="y">false</visibility>
  </panel>

  <model>
   <path>Aircraft/f16/Models/baseline.xml</path>
  </model>

  <view>
   <internal archive="y">true</internal>
   <config>
     <pitch-offset-deg>-10.0</pitch-offset-deg>
     <x-offset-m archive="y">0.0</x-offset-m>
     <y-offset-m archive="y">0.93</y-offset-m>
     <z-offset-m archive="y">-4.15</z-offset-m>
   </config>
  </view>

  <help>
   <title>General Dynamics F-16AT (Falcon-21)</title>
  </help>
 </sim>

</PropertyList>
