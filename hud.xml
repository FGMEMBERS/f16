<?xml version="1.0"?>

<PropertyList>

<!-- clipping is defined in f16-set.xml -->
<!-- dimensions are x -70 x 70 y 40 y -140 -->

    <enable3d>true</enable3d>
    
    <label>
        <name>G Load</name>
        <x>-43</x>
        <y>-20</y>
        <width>1</width>
        <height>1</height>
        <input>
            <property>/accelerations/pilot/z-accel-fps_sec</property>
            <factor>-0.03108095</factor>
            <damp>1.0</damp>
        </input>
        <format>%2.1f</format>
        <halign>left</halign>
    </label>

    <ladder>
        <name>Pitch Ladder</name>
        <type>pitch</type>
        <x>-20</x>
        <y>-120</y>
        <width>40</width>
        <height>239</height>
        <roll-input>
            <property>/orientation/roll-deg</property>
        </roll-input>
        <pitch-input>
            <property>/orientation/pitch-deg</property>
        </pitch-input>
        <compression-factor>11.63</compression-factor>
        <display-span>30.0</display-span>
        <divisions>5.0</divisions>
        <screen-hole>10</screen-hole>
        <zero-bar-overlength>100</zero-bar-overlength>
        <enable-dive-bar-angle>false</enable-dive-bar-angle>
        <enable-dynamic-origin>false</enable-dynamic-origin>
        <enable-fuselage-ref-line>false</enable-fuselage-ref-line>
        <enable-target-spot>false</enable-target-spot>
        <enable-velocity-vector>true</enable-velocity-vector>
        <enable-drift-marker>false</enable-drift-marker>
        <enable-alpha-bracket>false</enable-alpha-bracket>
        <enable-energy-marker>false</enable-energy-marker>
        <enable-climb-dive-marker>false</enable-climb-dive-marker>
        <enable-glide-slope-marker>false</enable-glide-slope-marker>
        <glide-slope>0.0</glide-slope>
        <enable-energy-worm>false</enable-energy-worm>
        <enable-waypoint-marker>false</enable-waypoint-marker>
    </ladder>

    <tape>
        <name>Altitude-MSL</name>
        <x>37</x>
        <y>-76</y>
        <width>7</width>
        <height>50</height>
        <input>
            <property>/position/altitude-ft</property>
            <factor>0.001</factor>
            <max>100</max>
            <min>-100</min>
        </input>
        <major-divisions>1</major-divisions>
        <minor-divisions>0.1</minor-divisions>
        <display-span>2.0</display-span>
        <option>right</option>
        <option>vertical</option>
        <tick-bottom>false</tick-bottom>
        <tick-top>false</tick-top>
        <tick-right>false</tick-right>
        <tick-left>false</tick-left>
        <cap-bottom>false</cap-bottom>
        <cap-top>false</cap-top>
        <cap-right>false</cap-right>
        <cap-left>false</cap-left>
        <marker-offset>5</marker-offset>
        <enable-pointer>true</enable-pointer>
        <pointer-type>fixed</pointer-type>
    </tape>

    <tape>
        <name>KIAS</name>
        <x>-50</x>
        <y>-76</y>
        <width>7</width>
        <height>50</height>
        <input>
            <property>/velocities/airspeed-kt</property>
            <factor>0.1</factor>
            <max>85</max>
            <min>0</min>
        </input>
        <major-divisions>1</major-divisions>
        <minor-divisions>0.1</minor-divisions>
        <display-span>2.0</display-span>
        <option>left</option>
        <option>vertical</option>
        <tick-bottom>false</tick-bottom>
        <tick-top>false</tick-top>
        <tick-right>false</tick-right>
        <tick-left>false</tick-left>
        <cap-bottom>false</cap-bottom>
        <cap-top>false</cap-top>
        <cap-right>false</cap-right>
        <cap-left>false</cap-left>
        <marker-offset>5</marker-offset>
        <enable-pointer>true</enable-pointer>
        <pointer-type>fixed</pointer-type>
    </tape>


    <tape>
        <name>Gyrocompass</name>
        <x>-14.8</x>
        <y>-93</y>
        <width>30</width>
        <height>10</height>
        <input>
            <property>/orientation/heading-deg</property>
        </input>
        <display-span>25.0</display-span>
        <modulo>360</modulo>
        <option>bottom</option>
        <major-divisions>10</major-divisions>
        <minor-divisions>5</minor-divisions>
        <tick-bottom>false</tick-bottom>
        <tick-top>true</tick-top>
        <tick-right>false</tick-right>
        <tick-left>false</tick-left>
        <cap-bottom>false</cap-bottom>
        <cap-top>false</cap-top>
        <cap-right>false</cap-right>
        <cap-left>false</cap-left>
        <marker-offset>0</marker-offset>
        <enable-pointer>false</enable-pointer>
        <pointer-type>fixed</pointer-type>
        <tick-type>line</tick-type>
        <tick-length>fixed</tick-length>
    </tape>


    <label>
        <name>KIAS</name>
        <x>-65</x>
        <y>-55</y>
        <width>18</width>
        <height>8</height>
        <input>
            <property>/velocities/airspeed-kt</property>
        </input>
        <format>%4.0f</format>
        <halign>left</halign>
        <box>true</box>
        <option>right</option>
    </label>

    <label>
        <name>KIAS Marker</name>
        <x>-43</x>
        <y>-47</y>
        <width>1</width>
        <height>1</height>
        <format>C</format>
        <halign>left</halign>
    </label>

    <label>
        <name>Altitude</name>
        <x>45</x>
        <y>-55</y>
        <width>24</width>
        <height>8</height>
        <input>
            <property>/position/altitude-ft</property>
        </input>
        <format>%4.0f</format>
        <halign>right</halign>
        <box>true</box>
        <option>left</option>
    </label>

    <label>
        <name>SIM</name>
        <x>-43</x>
        <y>-81</y>
        <width>1</width>
        <height>1</height>
        <format>SIM</format>
        <halign>left</halign>
    </label>


    <label>
        <name>MACH Number</name>
        <x>-43</x>
        <y>-87</y>
        <width>1</width>
        <height>1</height>
        <input>
            <property>/velocities/mach</property>
        </input>
        <format>%4.2f</format>
        <halign>left</halign>
    </label>

