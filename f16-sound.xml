<?xml version="1.0"?>
 
<PropertyList>

 <fx>
  <canopy>
   <name>canopy</name>
   <path>Aircraft/f16/Sounds/canopy.wav</path>
   <mode>in-transit</mode>
   <property>fdm/jsbsim/fcs/canopy-pos-norm</property>
   <reference-dist>30</reference-dist>
   <max-dist>100</max-dist>
   <position>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.195</z>
   </position>
   <pitch>
    <internal>dt_start</internal>
    <offset>1.25</offset>
    <factor>-0.25</factor>
   </pitch>
   <volume>
    <max>0.5</max>
   </volume>
  </canopy>

  <canopy>
   <name>canopy_lock</name>
   <path>Aircraft/f16/Sounds/canopy_lock.wav</path>
   <mode>single</mode>
   <condition>
    <equals>
     <property>fdm/jsbsim/fcs/canopy-pos-norm</property>
     <value>0.0</value>
    </equals>
   </condition>
   <reference-dist>10</reference-dist>
   <max-dist>40</max-dist>
   <position>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.195</z>
   </position>
   <volume>
    <max>0.5</max>
   </volume>
  </canopy>

  <engine_fan>
   <name>internal fan</name>
   <path>Sounds/whine.wav</path>
   <mode>looped</mode>
   <condition>
    <property>sim/current-view/internal</property>
    <property>engines/engine/running</property>
   </condition>
   <reference-dist>30</reference-dist>
   <max-dist>100</max-dist>
   <position>
    <x>-0.6</x>
    <y>0.0</y>
    <z>-0.37</z>
   </position>
   <orientation>
    <x>-0.98</x>
    <y>0.0</y>
    <z>0.0</z>
   </orientation>
   <pitch>
    <property>engines/engine/n1</property>
    <ofset>0.5</ofset>
    <factor>.00075</factor>
   </pitch>
   <volume>
    <factor>0.3</factor>
   </volume>
  </engine_fan>
  <engine_fan>
   <name>external fan</name>
   <path>Sounds/turbojet_fan.wav</path>
   <mode>looped</mode>
   <condition>
    <property>engines/engine/running</property>
    <not>
     <property>sim/current-view/internal</property>
    </not>
    <less-than>
      <property>engines/engine/n1</property>
      <value>85.0</value>
    </less-than>
   </condition>
   <reference-dist>800</reference-dist>
   <max-dist>3000</max-dist>
   <position>
    <x>0.6</x>
    <y>0.0</y>
    <z>-0.37</z>
   </position>
   <orientation>
    <x>-0.98</x>
    <y>0.0</y>
    <z>-0.17</z>
    <inner-angle>60.0</inner-angle>
    <outer-angle>200.0</outer-angle>
    <outer-gain>0.0</outer-gain>
   </orientation>
   <pitch>
    <property>engines/engine/n1</property>
    <ofset>0.8</ofset>
    <factor>.003</factor>
   </pitch>
   <volume>
    <property>engines/engine/n1</property>
    <offset>0.02</offset>
    <factor>-0.0002</factor>
   </volume>
  </engine_fan>
  <exhaust>
   <name>turbine</name>
   <path>Sounds/jet.wav</path>
   <mode>looped</mode>
   <property>engines/engine/running</property>
   <reference-dist>2000</reference-dist>
   <max-dist>10000</max-dist>
   <position>
    <x>4.6</x>
    <y>0.0</y>
    <z>0.0</z>
   </position>
   <orientation>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
    <inner-angle>200.0</inner-angle>
    <outer-angle>360.0</outer-angle>
    <outer-gain>0.2</outer-gain>
   </orientation>
   <volume>
    <property>engines/engine/thrust_lb</property>
    <factor>0.000044944</factor>
   </volume>
   <pitch>
    <property>engines/engine/n1</property>
    <offset>0.3</offset>
    <factor>0.01</factor>
   </pitch>
  </exhaust>

  <afterburner>
   <name>afterburner</name>
   <path>Sounds/exhaust_loop.wav</path>
   <mode>looped</mode>
   <property>engines/engine/augmentation</property>
   <reference-dist>5000</reference-dist>
   <max-dist>30000</max-dist>
   <position>
    <x>4.6</x>
    <y>0.0</y>
    <z>0.0</z>
   </position>
   <orientation>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
    <inner-angle>200.0</inner-angle>
    <outer-angle>360.0</outer-angle>
    <outer-gain>0.2</outer-gain>
   </orientation>
   <volume>
    <property>engines/engine/nozzle-pos-norm</property>
    <offset>0.05</offset>
    <factor>0.95</factor>
   </volume>
  </afterburner>

  <rumble>
   <name>gear rumble</name>
   <mode>looped</mode>
   <path>Sounds/rumble.wav</path>
   <condition>
    <property>sim/current-view/internal</property>
    <or>
     <property>gear/gear[1]/wow</property>
     <property>gear/gear[2]/wow</property>
    </or>
   </condition>
   <reference-dist>30</reference-dist>
   <max-dist>100</max-dist>
   <position>
    <x>1.2</x>
    <y>0.0</y>
    <z>-0.6</z>
   </position>
   <volume>
    <property>velocities/airspeed-kt</property>
    <factor>.004</factor>
   </volume>
  </rumble>

  <squeal>
   <name>squeal</name>
   <path>Sounds/squeal.wav</path>
   <condition>
    <not>
     <property>sim/current-view/internal</property>
    </not>
    <or>
     <property>gear/gear[1]/wow</property>
     <property>gear/gear[2]/wow</property>
    </or>
   </condition>
   <reference-dist>150</reference-dist>
   <max-dist>500</max-dist>
   <position>
    <x>1.2</x>
    <y>0.0</y>
    <z>-0.6</z>
   </position>
   <volume>
    <property>velocities/speed-down-fps</property>
    <factor>0.05</factor>
   </volume>
   <volume>
    <property>velocities/airspeed-kt</property>
    <factor>0.01</factor>
   </volume>
   <volume>
    <internal>dt_stop</internal>
    <factor>0.05</factor>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>velocities/airspeed-kt</property>
    <factor>0.0025</factor>
    <offset>1.2</offset>
   </pitch>
  </squeal>
 
  <stall>
   <name>stall</name>
   <path>Aircraft/f16/Sounds/warning.wav</path>
   <mode>looped</mode>
   <type>avionics</type>
   <condition>
    <and>
     <property>sim/alarms/stall-warning</property>
     <less-than>
      <property>velocities/airspeed-kt</property>
      <value>130.0</value>
     </less-than>
     <not>
      <property>gear/gear[1]/wow</property>
      <property>gear/gear[2]/wow</property>
     </not>
    </and>
   </condition>
  </stall>

  <cannon>
   <name>cannon</name>
   <path>Sounds/gatling.wav</path>
   <property>controls/armament/trigger</property>
   <mode>looped</mode>
   <position>
    <x>-1.0</x>
    <y>0.8</y>
    <z>0.0</z>
   </position>
  </cannon>
</fx>
 
</PropertyList>
