<?xml version="1.0"?>
 
<PropertyList>

 <fx>

  <engine>
   <name>internal fan</name>
   <path>Sounds/whine.wav</path>
   <mode>looped</mode>
   <condition>
    <property>sim/current-view/internal</property>
    <property>engines/engine/running</property>
   </condition>
   <position>
    <x>0.6</x>
    <y>0.0</y>
    <z>-0.37</z>
   </position>
   <orientation>
    <x>-0.98</x>
    <y>0.0</y>
    <z>-0.17</z>
    <inner-angle>80.0</inner-angle>
    <outer-angle>100.0</outer-angle>
    <outer-gain>0.0</outer-gain>
   </orientation>
   <pitch>
    <property>engines/engine/n1</property>
    <ofset>0.5</ofset>
    <factor>.00075</factor>
   </pitch>
   <volume>
    <factor>0.6</factor>
   </volume>
  </engine>

  <engine>
   <name>external fan</name>
   <path>Sounds/turbojet_fan.wav</path>
   <mode>looped</mode>
   <condition>
    <property>engines/engine/running</property>
    <not>
     <property>sim/current-view/internal</property>
    </not>
    <less-than>
      <property>engines/engine/n1</property>
      <value>85.0</value>
    </less-than>
   </condition>
   <position>
    <x>0.6</x>
    <y>0.0</y>
    <z>-0.37</z>
   </position>
   <orientation>
    <x>-0.98</x>
    <y>0.0</y>
    <z>-0.17</z>
    <inner-angle>80.0</inner-angle>
    <outer-angle>100.0</outer-angle>
    <outer-gain>0.0</outer-gain>
   </orientation>
   <pitch>
    <property>engines/engine/n1</property>
    <ofset>0.7</ofset>
    <factor>.003</factor>
   </pitch>
   <volume>
    <property>engines/engine/n1</property>
    <offset>0.02</offset>
    <factor>-0.0002</factor>
   </volume>
  </engine>

  <engine>
   <name>turbine</name>
   <path>Sounds/jet.wav</path>
   <mode>looped</mode>
   <property>engines/engine/running</property>
   <position>
    <x>-4.6</x>
    <y>00</y>
    <z>0.0</z>
   </position>
   <orientation>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
    <inner-angle>100.0</inner-angle>
    <outer-angle>180.0</outer-angle>
    <outer-gain>0.2</outer-gain>
   </orientation>
   <volume>
    <property>engines/engine/thrust_lb</property>
    <factor>0.000056179</factor>
   </volume>
   <pitch>
    <property>engines/engine/n1</property>
    <offset>0.3</offset>
    <factor>0.01</factor>
   </pitch>
  </engine>

  <engine>
   <name>afterburner</name>
   <path>Sounds/exhaust_loop.wav</path>
   <mode>looped</mode>
   <property>engines/engine/running</property>
   <position>
    <x>-4.6</x>
    <y>0.0</y>
    <z>0.0</z>
   </position>
   <orientation>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
    <inner-angle>100.0</inner-angle>
    <outer-angle>160.0</outer-angle>
    <outer-gain>0.1</outer-gain>
   </orientation>
   <volume>
    <property>engines/engine/augmentation</property>
    <offset>0.05</offset>
    <factor>0.95</factor>
   </volume>
  </engine>

  <rumble>
   <name>gear rumble</name>
   <mode>looped</mode>
   <path>Sounds/rumble.wav</path>
   <condition>
    <property>sim/current-view/internal</property>
    <or>
     <property>gear/gear[1]/wow</property>
     <property>gear/gear[2]/wow</property>
    </or>
   </condition>
   <position>
    <x>1.2</x>
    <y>0.0</y>
    <z>-0.6</z>
   </position>
   <volume>
    <property>velocities/airspeed-kt</property>
    <factor>.004</factor>
   </volume>
  </rumble>

  <squeal>
   <name>squeal</name>
   <path>Sounds/squeal.wav</path>
   <condition>
    <not>
     <property>sim/current-view/internal</property>
    </not>
    <or>
     <property>gear/gear[1]/wow</property>
     <property>gear/gear[2]/wow</property>
    </or>
   </condition>
   <position>
    <x>1.2</x>
    <y>0.0</y>
    <z>-0.6</z>
   </position>
   <volume>
    <property>velocities/speed-down-fps</property>
    <factor>0.05</factor>
   </volume>
   <volume>
    <property>velocities/airspeed-kt</property>
    <factor>0.01</factor>
   </volume>
   <volume>
    <internal>dt_stop</internal>
    <factor>0.05</factor>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>velocities/airspeed-kt</property>
    <factor>0.0025</factor>
    <offset>1.2</offset>
   </pitch>
  </squeal>
 
  <stall>
   <name>stall</name>
   <path>Sounds/stall.wav</path>
   <property>sim/alarms/stall-warning</property>
   <condition>
    <less-than>
     <property>velocities/airspeed-kt</property>
     <value>120.0</value>
    </less-than>
    <not>
     <property>gear/gear[1]/wow</property>
     <property>gear/gear[2]/wow</property>
    </not>
   </condition>
   <volume>
    <property>velocities/airspeed-kt</property>
    <max>0.3</max>
   </volume>
   <pitch>
    <factor>0.5</factor>
   </pitch>
  </stall>
 
</fx>
 
</PropertyList>
