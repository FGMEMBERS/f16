<?xml version="1.0"?>
 
<PropertyList>

 <fx>

  <engine>
   <name>engine</name>
   <path>Sounds/exhaust_loop.wav</path>
   <mode>looped</mode>
   <property>/engines/engine/running</property>
   <position>
    <x>0.0</x>
    <y>-4.60</y>
    <z>0.0</z>
   </position>
   <orientation>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
    <inner-angle>200.0</inner-angle>
    <outer-angle>180.0</outer-angle>
    <outer-gain>0.33</outer-gain>
   </orientation>
<!--
   <volume>
    <property>/engines/engine/thrust_lb</property>
    <factor>0.000034451</factor>
   </volume>
-->
  </engine>

  <engine>
   <name>engine</name>
   <path>Sounds/jet.wav</path>
   <mode>looped</mode>
   <property>engines/engine/augmentation</property>
   <position>
    <x>0.0</x>
    <y>-4.60</y>
    <z>0.0</z>
   </position>
   <orientation>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
    <inner-angle>100.0</inner-angle>
    <outer-angle>180.0</outer-angle>
    <outer-gain>0.2</outer-gain>
   </orientation>
  </engine>

  <engine>
   <name>external whine</name>
   <path>Sounds/turbojet_fan.wav</path>
   <mode>looped</mode>
   <condition>
    <not>
     <property>/sim/current-view/internal</property>
    </not>
    <property>/engines/engine/running</property>
   </condition>
   <position>
    <x>0.37</x>
    <y>-0.60</y>
    <z>0.0</z>
   </position>
   <orientation>
    <x>0.03</x>
    <y>1.0</y>
    <z>0.0</z>
    <inner-angle>80.0</inner-angle>
    <outer-angle>100.0</outer-angle>
    <outer-gain>0.0</outer-gain>
   </orientation>
   <pitch>
    <property>/engines/engine/n1</property>
    <ofset>0.7</ofset>
    <factor>.003</factor>
   </pitch>
   <volume>
    <property>/engines/engine/n1</property>
    <offset>0.4</offset>
    <factor>-0.004</factor>
   </volume>
  </engine>

  <engine>
   <name>internal whine</name>
   <path>Sounds/whine.wav</path>
   <mode>looped</mode>
   <condition>
    <property>/sim/current-view/internal</property>
    <property>/engines/engine/running</property>
   </condition>
   <position>
    <x>0.37</x>
    <y>-0.60</y>
    <z>0.0</z>
   </position>
   <orientation>
    <x>0.03</x>
    <y>1.0</y>
    <z>0.0</z>
    <inner-angle>80.0</inner-angle>
    <outer-angle>100.0</outer-angle>
    <outer-gain>0.0</outer-gain>
   </orientation>
   <pitch>
    <property>/engines/engine/n1</property>
    <ofset>0.5</ofset>
    <factor>.00075</factor>
   </pitch>
   <volume>
    <max>0.6</max>
   </volume>
  </engine>

  <rumble>
   <name>gear rumble</name>
   <mode>looped</mode>
   <path>Sounds/rumble.wav</path>
   <condition>
    <property>/sim/current-view/internal</property>
    <or>
     <property>/gear/gear[0]/wow</property>
     <property>/gear/gear[1]/wow</property>
     <property>/gear/gear[2]/wow</property>
    </or>
   </condition>
   <position>
    <x>0.6</x>
    <y>-1.2</y>
    <z>0.0</z>
   </position>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <factor>.004</factor>
   </volume>
  </rumble>

  <squeal>
   <name>squeal</name>
   <path>Sounds/squeal.wav</path>
   <condition>
    <not>
     <property>/sim/current-view/internal</property>
    </not>
    <or>
     <property>/gear/gear[0]/wow</property>
     <property>/gear/gear[1]/wow</property>
     <property>/gear/gear[2]/wow</property>
    </or>
   </condition>
   <position>
    <x>0.6</x>
    <y>-1.2</y>
    <z>-0.60</z>
   </position>
   <volume>
    <property>/velocities/speed-down-fps</property>
    <factor>0.05</factor>
   </volume>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <factor>0.01</factor>
   </volume>
   <volume>
    <internal>dt_stop</internal>
    <factor>0.05</factor>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>/velocities/airspeed-kt</property>
    <factor>0.0025</factor>
    <offset>1.2</offset>
   </pitch>
  </squeal>
 
  <stall>
   <name>stall</name>
   <path>Sounds/stall.wav</path>
   <property>/sim/alarms/stall-warning</property>
   <condition>
    <property>/sim/current-view/internal</property>
   </condition>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <max>0.3</max>
   </volume>
   <pitch>
    <max>0.5</max>
   </pitch>
  </stall>
 
</fx>
 
</PropertyList>
